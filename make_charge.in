variable ionatoms atom "random(0, 1, 34312)*(q*0+1)"
group ions empty
variable c loop 5
variable prostr index  8 4 3 2 1
label loopx
variable pro equal 0.${prostr}
print "${pro}"
variable inatoms atom "v_ionatoms < v_pro"
group temp1 region dcy$c
group temp2 variable inatoms
group ions intersect temp1 temp2 
variable na equal count(ions)
print "${na}"
variable inatoms delete

group temp1 delete
group temp2 delete
next c
next prostr
jump SELF loopx